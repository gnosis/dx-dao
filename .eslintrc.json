{
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": "off",
        "eol-last": 2,
        "eqeqeq": "error",
        "no-console": 0
    },
    "env": {
        "commonjs": true,
        "node": true,
        "es6": true
    }
}